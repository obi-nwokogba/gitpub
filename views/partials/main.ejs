<div class="container">
    <div class="row">
        <div class="col-sm">

        </div>
        <div class="col-sm">
            <%# Loops through drinks, displays and capitalizes first letter. %>
                <ul>Drinks
                    <% for(let i=0; i < drinks.length; i++){ %>
                        <li><a href="/drinks/<%=i%>">
                                <%=drinks[i].name %>
                            </a> </li>
                        <% } %>
                </ul>
                <ul>Food
                    <% for(let i=0; i < food.length; i++){ %>
                        <li><a href="/food/<%=i%>">
                                <%=food[i].name%>
                            </a> </li>
                        <% } %>
                </ul>
        </div>
        <div class="col-sm">

        </div>
    </div>
</div